{% trans_default_domain 'FOSUserBundle' %}


<form action="{{ path('fos_user_resetting_reset', {'token': token}) }}" {{ form_enctype(form) }} method="POST" class="form-horizontal fos_user_resetting_reset">
    {{ form_widget(form) }}

    <div class="col-md-12" >
        <div class="pull-right">
            <input type="submit" value="{{ 'resetting.reset.submit'|trans }}" class="btn btn-primary" />
        </div>

    </div>
</form>

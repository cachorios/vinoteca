{#{{ form_widget(form.metadatos) }}#}


<ul id="email-fields-list" data-prototype="{{ form_widget(form.metadatos.vars.prototype)|e }}">
    {% for metadataField in form.metadatos %}
        <li>
            {#{{ form_errors(metadataField) }}#}
            {{ form_widget(metadataField.nombre) }}
            {{ form_widget(metadataField.orden) }}
            {{ form_widget(metadataField.listaValores) }}
            {{ form_widget(metadataField.filtrable) }}
        </li>
    {% endfor %}
</ul>